<vosao-plugin>
    <name>analytics</name>
    <title>Google Analytics</title>
    <version>1.0</version>
    <description>Google Analytics integration</description>
    <website>www.nexoft.ru</website>
    <config>
        <param name="ga-user" title="$analytics.username" type="String" value="" />
